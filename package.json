    "react-router-scroll-memory": "^2.0.7",
    "react-syntax-highlighter": "^15.4.3",
    "react-use": "^17.0.1",
    "renovate-config-seek": "0.4.0",
    "sanitize-html": "^2.3.2",
    "sku": "10.9.4",
    "surge": "^0.21.7",
    "title-case": "^3.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.0"
  },
  "resolutions": {
    "playroom": "0.23.0"
  },
  "volta": {
    "node": "12.14.1",
    "yarn": "1.21.1"
  }
}
