  "description": "Themeable design system for the SEEK Group",
  "main": "lib/components/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "yarn check --integrity && npm run lint && CI=true npm run unit && npm run build",
    "unit": "sku test",
    "build": "sku build",
    "start": "sku start",
    "lint": "sku lint",
    "format": "sku format",
    "commit": "git-cz",
    "commitmsg": "commitlint --edit --extends seek",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/braid-design-system.git"
  },
  "author": "SEEK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seek-oss/braid-design-system/issues"
  },
  "homepage": "https://github.com/seek-oss/braid-design-system#readme",
  "dependencies": {
    "basekick": "^3.0.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "commitizen": "^2.10.1",
    "commitlint-config-seek": "1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "dedent": "0.7.0",
    "deep-keys": "^0.5.0",
    "husky": "^1.0.1",
    "lodash": "^4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-testing-library": "^5.1.0",
    "renovate-config-seek": "0.4.0",
    "semantic-release": "^15.9.16",
    "sku": "5.10.1",
    "travis-deploy-once": "^5.0.9"
  }
