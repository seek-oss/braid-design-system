{
  "name": "braid",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "chromatic": "nx run braid-design-system:chromatic",
    "test": "jest",
    "start": "nx run braid-site:start",
    "start-playroom": "nx run braid-site:playroom",
    "lint": "pnpm manypkg:check && pnpm manypkg:depcheck && pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "manypkg:check": "manypkg check",
    "manypkg:depcheck": "manypkg exec depcheck --ignore-patterns=dist --ignores=braid-design-system,generate-component-docs,@svgr/plugin-jsx,@svgr/plugin-prettier,assert,@types/jest,@testing-library/dom,@seek/braid-codemod",
    "eslint": "nx run-many --target eslint --all",
    "prettier": "nx run-many --target prettier --all",
    "tsc": "nx run-many --target tsc --all",
    "format": "nx run-many --target format --all",
    "generate-component-docs": "nx run braid-site:generate-component-docs",
    "generate-icons": "nx run braid-design-system:generate-icons",
    "build-site": "nx run braid-site:build",
    "storybook": "nx run braid-design-system:storybook",
    "build-storybook": "nx run braid-design-system:build-storybook",
    "deploy": "node scripts/deploy.js",
    "post-commit-status": "node scripts/postCommitStatus.js",
    "release": "pnpm prepare-publish && changeset publish && pnpm build-site && pnpm run deploy",
    "prepare-publish": "nx run braid-design-system:build",
    "version": "changeset version && ts-node scripts/versionComponentUpdates",
    "changeset": "EDITOR=scripts/writeBraidFrontMatter.js ./node_modules/.bin/changeset add --open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/braid-design-system.git"
  },
  "author": "SEEK",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.18.0",
    "@changesets/cli": "^2.22.0",
    "@changesets/get-github-info": "^0.5.0",
    "@manypkg/cli": "^0.19.1",
    "@octokit/rest": "^18.12.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/sanitize-html": "^2.6.2",
    "@types/webpack-env": "^1.17.0",
    "@vanilla-extract/babel-plugin": "^1.1.7",
    "babel-jest": "^28.1.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-treat": "^1.6.2",
    "depcheck": "^1.4.3",
    "enquirer": "^2.3.6",
    "escape-string-regexp": "^4.0.0",
    "eslint": "^8.21.0",
    "eslint-config-seek": "9.0.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gh-pages": "^4.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-watch-typeahead": "^2.0.0",
    "js-yaml": "^4.1.0",
    "nx": "^14.5.3",
    "prettier": "^2.6.2",
    "renovate-config-seek": "0.4.0",
    "surge": "^0.23.1",
    "ts-node": "^10.8.0",
    "typescript": "4.4.4"
  },
  "volta": {
    "node": "16.13.0"
  },
  "packageManager": "pnpm@7.8.0",
  "pnpm": {
    "patchedDependencies": {
      "panzoom@9.4.2": "patches/panzoom@9.4.2.patch",
      "sku@11.4.5": "patches/sku@11.4.5.patch"
    }
  },
  "skuSkipPostInstall": true
}
