{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/15.0.0/packages/nx/schemas/project-schema.json",
  "targets": {
    "start": {
      "dependsOn": ["start-site", "playroom"],
      "executor": "nx:noop"
    },
    "build": {
      "dependsOn": ["build-site", "build-playroom"],
      "executor": "nx:noop"
    },
    "start-site": {
      "dependsOn": ["^build"],
      "executor": "nx:run-script",
      "options": {
        "script": "start-site"
      }
    },
    "build-site": {
      "dependsOn": ["^build"],
      "executor": "nx:run-script",
      "options": {
        "script": "build-site"
      }
    }
  },
  "implicitDependencies": ["braid-design-system"]
}
