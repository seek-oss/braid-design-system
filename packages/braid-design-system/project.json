{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/15.0.0/packages/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "dependsOn": ["copy-codemod", "generate-icons"],
      "executor": "nx:noop"
    },
    "copy-codemod": {
      "dependsOn": ["^build"],
      "executor": "nx:run-script",
      "options": {
        "script": "copy-codemod"
      }
    },
    "chromatic": {
      "dependsOn": ["build-storybook"],
      "executor": "nx:run-script",
      "options": {
        "script": "chromatic"
      }
    }
  },
  "implicitDependencies": ["codemod"]
}
