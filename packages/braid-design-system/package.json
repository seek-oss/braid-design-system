{
  "name": "braid-design-system",
  "version": "34.0.2",
  "description": "Themeable design system for the SEEK Group",
  "homepage": "https://seek-oss.github.io/braid-design-system/",
  "bugs": {
    "url": "https://github.com/seek-oss/braid-design-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/braid-design-system.git",
    "directory": "packages/braid-design-system"
  },
  "license": "MIT",
  "author": "SEEK",
  "sideEffects": [
    "**/components/BraidProvider/BraidProvider.*",
    "**/playroom/components.ts",
    "**/reset.ts",
    "**/reset/**",
    "*.css.*",
    "dist/playroom/components.*",
    "dist/reset.*"
  ],
  "type": "module",
  "exports": {
    ".": {
      "braid-dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./color-mode/query-param": {
      "braid-dev": "./src/color-mode/query-param.ts",
      "require": "./dist/color-mode/query-param.cjs",
      "import": "./dist/color-mode/query-param.mjs"
    },
    "./css": {
      "braid-dev": "./src/css.ts",
      "require": "./dist/css.cjs",
      "import": "./dist/css.mjs"
    },
    "./playroom/components": {
      "braid-dev": "./src/playroom/components.ts",
      "require": "./dist/playroom/components.cjs",
      "import": "./dist/playroom/components.mjs"
    },
    "./playroom/FrameComponent": {
      "braid-dev": "./src/playroom/FrameComponent.ts",
      "require": "./dist/playroom/FrameComponent.cjs",
      "import": "./dist/playroom/FrameComponent.mjs"
    },
    "./playroom/scope": {
      "braid-dev": "./src/playroom/scope.ts",
      "require": "./dist/playroom/scope.cjs",
      "import": "./dist/playroom/scope.mjs"
    },
    "./playroom/snippets": {
      "braid-dev": "./src/playroom/snippets.ts",
      "require": "./dist/playroom/snippets.cjs",
      "import": "./dist/playroom/snippets.mjs"
    },
    "./reset": {
      "braid-dev": "./src/reset.ts",
      "require": "./dist/reset.cjs",
      "import": "./dist/reset.mjs"
    },
    "./test": {
      "braid-dev": "./src/test.ts",
      "require": "./dist/test.cjs",
      "import": "./dist/test.mjs"
    },
    "./themes/docs": {
      "braid-dev": "./src/themes/docs.ts",
      "require": "./dist/themes/docs.cjs",
      "import": "./dist/themes/docs.mjs"
    },
    "./themes/seekBusiness": {
      "braid-dev": "./src/themes/seekBusiness.ts",
      "require": "./dist/themes/seekBusiness.cjs",
      "import": "./dist/themes/seekBusiness.mjs"
    },
    "./themes/seekJobs": {
      "braid-dev": "./src/themes/seekJobs.ts",
      "require": "./dist/themes/seekJobs.cjs",
      "import": "./dist/themes/seekJobs.mjs"
    },
    "./themes/wireframe": {
      "braid-dev": "./src/themes/wireframe.ts",
      "require": "./dist/themes/wireframe.cjs",
      "import": "./dist/themes/wireframe.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "CHANGELOG.md",
    "color-mode",
    "color-mode/query-param",
    "css",
    "dist",
    "playroom/FrameComponent",
    "playroom/components",
    "playroom/scope",
    "playroom/snippets",
    "query-param",
    "reset",
    "test",
    "themes/docs",
    "themes/seekBusiness",
    "themes/seekJobs",
    "themes/wireframe"
  ],
  "scripts": {
    "%skott": "skott --displayMode=file-tree --showCircularDependencies --no-trackTypeOnlyDependencies",
    "build": "tsdown",
    "format:eslint": "eslint --cache --fix .",
    "format:prettier": "prettier --cache --write .",
    "generate:icons": "tsx ./scripts/generateIcons.cts",
    "generate:snippets": "tsx ./scripts/generateSnippets.cts",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --cache --list-different .",
    "lint:tsc": "tsc",
    "validate:dist": "tsx ./scripts/validateEsmImports.ts && pnpm %skott dist/index.mjs",
    "validate:src": "pnpm %skott src"
  },
  "dependencies": {
    "@capsizecss/core": "^4.1.2",
    "@capsizecss/metrics": "^3.0.0",
    "@capsizecss/vanilla-extract": "^2.0.4",
    "@floating-ui/react-dom": "^2.1.6",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.1.2",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "assert": "^2.1.0",
    "autosuggest-highlight": "^3.3.4",
    "clsx": "^2.1.1",
    "csstype": "^3.0.6",
    "dedent": "^1.5.3",
    "gradient-parser": "^1.0.0",
    "is-mobile": "^5.0.0",
    "lodash.curry": "^4.1.1",
    "polished": "^4.1.0",
    "react-focus-lock": "^2.9.7",
    "react-remove-scroll": "^2.7.0",
    "throttle-debounce": "^5.0.2",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-typescript": "^7.20.13",
    "@babel/types": "^7.26.0",
    "@braid-design-system/source.macro": "workspace:*",
    "@storybook/react-webpack5": "^10.2.7",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/autosuggest-highlight": "^3.1.1",
    "@types/babel__core": "^7.20.1",
    "@types/gradient-parser": "^0.1.5",
    "@types/lodash.curry": "^4.1.9",
    "@types/node": "^22.16.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sanitize-html": "^2.13.0",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/throttle-debounce": "^5.0.2",
    "@vanilla-extract/rollup-plugin": "^1.5.1",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-remove-unused-vars": "^2.2.0",
    "babel-plugin-transform-remove-imports": "^1.7.0",
    "change-case": "^4.1.2",
    "cheerio": "1.0.0-rc.11",
    "fast-glob": "^3.3.2",
    "fs-extra": "^10.1.0",
    "html-validate": "^9.7.1",
    "picocolors": "^1.1.1",
    "playroom": "1.0.7",
    "prettier": "^3.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanitize-html": "^2.12.1",
    "storybook": "^10.2.7",
    "svgo": "^2.8.0",
    "title-case": "^3.0.3"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      },
      "./color-mode/query-param": {
        "require": "./dist/color-mode/query-param.cjs",
        "import": "./dist/color-mode/query-param.mjs"
      },
      "./css": {
        "require": "./dist/css.cjs",
        "import": "./dist/css.mjs"
      },
