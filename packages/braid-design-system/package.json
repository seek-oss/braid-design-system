{
  "name": "braid-design-system",
  "version": "32.8.2",
  "description": "Themeable design system for the SEEK Group",
  "homepage": "https://seek-oss.github.io/braid-design-system/",
  "bugs": {
    "url": "https://github.com/seek-oss/braid-design-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/braid-design-system.git",
    "directory": "packages/braid-design-system"
  },
  "license": "MIT",
  "author": "SEEK",
  "sideEffects": [
    "**/atoms/**",
    "**/components/BraidProvider/BraidProvider.*",
    "**/entries/playroom/components.ts",
    "**/entries/reset.ts",
    "**/reset/**",
    "**/themes/**",
    "*.css.*",
    "dist/playroom/components.*",
    "dist/reset.*",
    "dist/side-effects/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./color-mode/query-param": {
      "types": "./dist/color-mode/query-param.d.ts",
      "import": "./dist/color-mode/query-param.mjs",
      "require": "./dist/color-mode/query-param.cjs"
    },
    "./css": {
      "types": "./dist/css.d.ts",
      "import": "./dist/css.mjs",
      "require": "./dist/css.cjs"
    },
    "./playroom/FrameComponent": {
      "types": "./dist/playroom/FrameComponent.d.ts",
      "import": "./dist/playroom/FrameComponent.mjs",
      "require": "./dist/playroom/FrameComponent.cjs"
    },
    "./playroom/components": {
      "types": "./dist/playroom/components.d.ts",
      "import": "./dist/playroom/components.mjs",
      "require": "./dist/playroom/components.cjs"
    },
    "./playroom/scope": {
      "types": "./dist/playroom/scope.d.ts",
      "import": "./dist/playroom/scope.mjs",
      "require": "./dist/playroom/scope.cjs"
    },
    "./playroom/snippets": {
      "types": "./dist/playroom/snippets.d.ts",
      "import": "./dist/playroom/snippets.mjs",
      "require": "./dist/playroom/snippets.cjs"
    },
    "./reset": {
      "types": "./dist/reset.d.ts",
      "import": "./dist/reset.mjs",
      "require": "./dist/reset.cjs"
    },
    "./test": {
      "types": "./dist/test.d.ts",
      "import": "./dist/test.mjs",
      "require": "./dist/test.cjs"
    },
    "./themes/apac": {
      "types": "./dist/themes/apac.d.ts",
      "import": "./dist/themes/apac.mjs",
      "require": "./dist/themes/apac.cjs"
    },
    "./themes/docs": {
      "types": "./dist/themes/docs.d.ts",
      "import": "./dist/themes/docs.mjs",
      "require": "./dist/themes/docs.cjs"
    },
    "./themes/seekBusiness": {
      "types": "./dist/themes/seekBusiness.d.ts",
      "import": "./dist/themes/seekBusiness.mjs",
      "require": "./dist/themes/seekBusiness.cjs"
    },
    "./themes/seekJobs": {
      "types": "./dist/themes/seekJobs.d.ts",
      "import": "./dist/themes/seekJobs.mjs",
      "require": "./dist/themes/seekJobs.cjs"
    },
    "./themes/wireframe": {
      "types": "./dist/themes/wireframe.d.ts",
      "import": "./dist/themes/wireframe.mjs",
      "require": "./dist/themes/wireframe.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "braid-upgrade": "./codemod/bin.js"
  },
  "files": [
    "CHANGELOG.md",
    "codemod",
    "color-mode",
    "color-mode/query-param",
    "css",
    "dist",
    "playroom/FrameComponent",
    "playroom/components",
    "playroom/scope",
    "playroom/snippets",
    "query-param",
    "reset",
    "test",
    "themes/apac",
    "themes/docs",
    "themes/seekBusiness",
    "themes/seekJobs",
    "themes/wireframe"
  ],
  "scripts": {
    "build": "crackle package",
    "codemod": "rm -rf ./codemod/dist && cp -r ../codemod/dist ./codemod/dist",
    "preformat:eslint": "ignore-sync .eslintignore-sync",
    "format:eslint": "eslint --cache --fix .",
    "format:prettier": "prettier --cache --write .",
    "generate:icons": "tsx ./scripts/generateIcons.cts",
    "generate:snippets": "tsx ./scripts/generateSnippets.cts",
    "prelint:eslint": "ignore-sync .eslintignore-sync",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --cache --list-different .",
    "lint:tsc": "tsc",
    "storybook": "SKU_TELEMETRY=false sku storybook",
    "storybook:build": "SKU_TELEMETRY=false sku build-storybook",
    "storybook:chromatic": "chromatic --storybook-build-dir ./dist-storybook --exit-zero-on-changes --exit-once-uploaded --auto-accept-changes master"
  },
  "dependencies": {
    "@capsizecss/core": "^3.1.0",
    "@capsizecss/metrics": "^1.1.1",
    "@capsizecss/vanilla-extract": "^1.0.0",
    "@types/autosuggest-highlight": "^3.1.1",
    "@types/dedent": "^0.7.0",
    "@types/lodash": "^4.14.168",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "assert": "^2.0.0",
    "autosuggest-highlight": "^3.1.1",
    "clsx": "^1.1.1",
    "csstype": "^3.0.6",
    "dedent": "^0.7.0",
    "gradient-parser": "^1.0.0",
    "is-mobile": "^2.2.2",
    "lodash": "^4.17.21",
    "polished": "^4.1.0",
    "react-focus-lock": "^2.9.1",
    "react-is": "^18.2.0",
    "react-popper-tooltip": "^4.3.1",
    "react-remove-scroll": "^2.5.3",
    "utility-types": "^3.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/generator": "^7.18.0",
    "@babel/plugin-transform-typescript": "^7.20.13",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/babel-plugin-macros": "^2.8.5",
    "@types/babel-plugin-tester": "^9.0.5",
    "@types/babel__core": "^7.18.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-is": "^17.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/sanitize-html": "^2.6.2",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/uuid": "^8.3.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-tester": "^10.1.0",
    "babel-plugin-transform-remove-imports": "^1.7.0",
    "change-case": "^4.1.2",
    "cheerio": "1.0.0-rc.11",
    "chromatic": "^5.10.2",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "html-validate": "^7.1.1",
    "playroom": "0.31.0",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "sanitize-html": "^2.7.0",
    "sku": "12.1.2",
    "svgo": "^2.8.0",
    "title-case": "^3.0.3"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18",
    "sku": ">=10.13.1"
  },
  "skuSkipConfigure": true,
  "skuSkipPostInstall": true,
  "skuSkipValidatePeerDeps": true
}
