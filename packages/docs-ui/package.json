{
  "name": "@braid-design-system/docs-ui",
  "version": "4.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/braid-design-system.git",
    "directory": "packages/docs-ui"
  },
  "license": "MIT",
  "author": "SEEK",
  "type": "module",
  "exports": {
    ".": {
      "braid-dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "%skott": "skott --displayMode=file-tree --showCircularDependencies --no-trackTypeOnlyDependencies",
    "build": "tsdown",
    "format:eslint": "eslint --cache --fix .",
    "format:prettier": "prettier --cache --write .",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --cache --list-different .",
    "lint:tsc": "tsc",
    "validate:dist": "pnpm %skott dist/index.mjs",
    "validate:src": "pnpm %skott src"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.9.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@vanilla-extract/rollup-plugin": "^1.5.1",
    "braid-design-system": "workspace:*",
    "react": "^19.1.0"
  },
  "peerDependencies": {
    "braid-design-system": "^34.0.0",
    "react": "^18 || ^19"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      },
      "./package.json": "./package.json"
    }
  }
}
